    "bang",
    "attribute",
    "html",
    "dom",
    "plugin",
    "addon"
  ],
  "author": "Russell Steadman",
  "organization": "Russell Steadman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teamtofu/onebang/issues"
  },
  "homepage": "https://onebang.js.org/",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/teamtofu/onebang.git",
